*{
				margin:0;padding:0;
				/*text-align:center;*/
			}
			.out{
				height:200px;
				overflow:hidden;
			}
			ul.catalog{
				list-style:none;
				-moz-animation:fade-in 7.5s infinite 2s;
			}
			ul.catalog{
				 list-style:none;
				 -ms-animation:fade-in 7.5s infinite 2s;
			 }
			ul.catalog{
				list-style:none;
				-webkit-animation:fade-in 7.5s infinite 2s;
			}
			ul.catalog{
				list-style:none;
				-o-animation:fade-in 7.5s infinite 2s;
			}
			ul.catalog > li{
				float:left;
				margin:20px;
				margin-bottom:150px;
			}
			ul.catalog > li > div {
				width:40px;
				height:40px;
				background-color:#1e78b7;
				transform:rotate(45deg);
				box-shadow: inset 3px 3px 14px 0px #fff;
				/*border-radius:5px;*/
			}
			ul.catalog > li > div.sq-lg {
				transform:scale(1.5,1.5) rotate(45deg)
			}
			ul.catalog > li > div > div{
				transform:rotate(-45deg);
				width:1em;padding:1em;
				color:white;
				padding-left: 19px;
				padding-top: 3px;
			}
			ul.catalog > li > div.sq-lg > div{
				transform:rotate(-45deg);
				width:2em;padding:0.5em;
				line-height: 13px;
				margin-left: 3px;
				font-size: 12px;
			}
			ul.catalog > li > div.sq-lg+p{
				padding:0px;
			}
			ul.catalog > li > p{
				margin:12px auto;
				width:25px;
				background-color:#b4d5ea;
				border-radius:5px;
				padding:10px 2px;
				color: #013467;
				font-weight: bold;
				line-height: 17px;
				filter:alpha(opacity=80);
				-moz-opacity:0.8;
				-khtml-opacity: 0.8;
				opacity: 0.8;
				text-align: center;
			}

			@-moz-keyframes move{
				from {width:0px}
				to {width:100px}
			}
			@-ms-keyframes move{
				from {width:0px}
				to {width:100px}
			}
			@-webkit-keyframes move{
				from {width:0px}
				to {width:100px}
			}
			@-o-keyframes move{
				from {width:0px}
				to {width:100px}
			}
			@keyframes fade-in {
				0% {opacity: 0;width:0%;}/*初始状态 透明度为0*/

				100% {opacity: 1;width:100%;}/*结束状态 透明度为1*/
			}
			@-moz-keyframes fade-in {/*针对Firefox*/
				0% {opacity: 0;width:0%}

				100% {opacity: 1;width:100%;}
			}
			@-ms-keyframes fade-in {/*针对IE*/
				0% {opacity: 0;width:0%}

				100% {opacity: 1;width:100%;}
			}
			@-webkit-keyframes fade-in {/*针对webkit内核*/
				0% {opacity: 0;width:0%}

				100% {opacity: 1;width:100%;}
			}
			@-o-keyframes fade-in {/*针对Opera*/
				0% {opacity: 0;width:0%}

				100% {opacity: 1;width:100%;}
			}

