<script>
	function xxkbjchangess(obj,objs,num)
	{
		var dhz=document.getElementById(obj);
		var dhli=dhz.getElementsByTagName('a').length;
		for(i=0; i<dhli; i++)
		{
			var bjid=objs+i;
			if(i==num)
